
#include <iostream>
using namespace std;
int main()
{
int n;
cin>>n;
int t = 1;
while(t*t<=n){
   cout <<t*t<<" ";
   t++;
}
return 0;
} //Задача первая: выводит все степени натур. чисел 


#include <iostream>
using namespace std;
int main()
{
int a;
cin>>a;
int i = 2;
while(a%i!=0){
   i++;
}
cout <<i<<" ";
return 0;
} //Задача вторая: выводит натуральный наименьший делитель числа

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
int i, s, a;
cin>>a;
i = 2;
s = 0;
while(pow(i,s)<=a){
   cout <<pow (i,s)<<" ";
   s=s+1;
}
} //Задача третья: выводит все степени двойки в данном числовом массиве

#include <iostream>
using namespace std;
int main(){
int n;
cin>>n;
int i = 1;
while(i%n != 0){
    i=i*2;
    
}
if (i == n){
    cout<<"YES";
    }
   else
{
    cout<<"NO";
}
} //Задача четвертая: проверяет введенное число на то степень ли она двойки

